{
  "name": "antd-vue3-pro",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build",
    "build:test": "cross-env vite build --mode test",
    "build:no-cache": "yarn clean:cache && npm run build",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx,js,jsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,jsx,json,ts,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "reinstall": "rimraf yarn.lock && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,jsx,json,ts,tsx,css,less,scss,vue,html,md}": "prettier --write",
    "*.{vue,ts,tsx,js,jsx}": "eslint --fix",
    "*.{vue,less,postcss,css,scss}": "stylelint --fix"
  },
  "dependencies": {
    "@ant-design/colors": "^7.0.0",
    "ant-design-vue": "^3.2.15",
    "dayjs": "^1.11.7",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "@vitest/coverage-c8": "^0.28.5",
    "@vitest/ui": "^0.28.5",
    "@vue/test-utils": "^2.2.10",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-define-config": "^1.15.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.0",
    "jsdom": "^21.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.1.2",
    "postcss": "^8.4.21",
    "postcss-html": "^1.5.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.8.4",
    "rimraf": "^4.1.2",
    "stylelint": "^15.1.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-standard": "^30.0.1",
    "stylelint-order": "^6.0.2",
    "typescript": "^4.9.3",
    "unplugin-vue-macros": "^1.8.1",
    "vite": "^4.1.0",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^0.28.5",
    "vue-eslint-parser": "^9.1.0",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": "^12 || >=14"
  }
}
